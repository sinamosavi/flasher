<Script>
function CheckDevice()
 devices = CommandLine.Execute("fastboot devices", 4000);
 search = String.Find(devices.StdOut, "fastboot", 1, false);
 if (search~=-1) then
  connected=true
  Shape.SetFillColor("Plugin1", 32768);
  Label.SetText("Label7", "Device connected");
 else
  connected=false
  Shape.SetFillColor("Plugin1", 65535);
  Label.SetText("Label7", "Device not connected");
 end
end

function AddLog(text)
 paragraph = Paragraph.GetText("Paragraph1");
 paragraph = paragraph.."\n"..text
 Paragraph.SetText("Paragraph1", paragraph);
end

function GetExtension(file)
 trim = String.SplitPath(file);
 return trim
end

function Fastboot()
 checkbox1=CheckBox.GetChecked("CheckBox1");
 if (checkbox1==true) then
  fastboot="fastboot -w"
 else
  fastboot="fastboot"
 end
 return fastboot
end




