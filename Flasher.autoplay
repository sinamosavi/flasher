<Event>
<Name>On Click</Name>
<Args/>
<Script>if (selected1~=true and selected2~=true and selected3~=true and selected4~=true and selected5~=true and selected6~=true ) then
 AddLog("You haven't selected any files to flash.");
 anyselected=false
end

if (connected~=true) then
 AddLog("You haven't plugged your device in.");
end

if (connected==true and anyselected~=false and RadioButton.GetChecked("RadioButton1")==true) then

 fastboot=Fastboot()
 if (selected1==true) then
 --Start detect
 kernel2=GetExtension(kernel[1])
 if (kernel2.Extension==".img") then
  kerneltype="boot"
 elseif (kernel2.Extension==".zip") then
  kerneltype="zip"
 elseif (kernel2.Filename=="zImage") then
  kerneltype="zimage"
 else
  kerneltype="unknown"
 end
 --Finish detect
 
 --Start flash
 
  if (kerneltype=="boot") then
  AddLog("Flashing kernel...");
  CommandLine.Execute(fastboot.." flash boot "..kernel[1], 0);
  AddLog("Flashed kernel successfully.");
 elseif (kerneltype=="zip") then
  AddLog("Flashing kernel...");
  Folder.Create(_TempFolder.."/Flasher");
  CommandLine.Execute("7za x -o".._TempFolder.."/Flasher "..kernel[1], 0);
  zimage = File.Find(_TempFolder.."\\Flasher", "zImage", true, false, nil, nil);
  zimage = zimage[1]
  CommandLine.Execute(fastboot.." flash zimage "..zimage, 0);
  AddLog("Flashed kernel successfully.");
  Folder.DeleteTree(_TempFolder.."\\Flasher", nil);
 elseif (kerneltype=="zimage") then
  AddLog("Flashing kernel...");
  CommandLine.Execute(fastboot.." flash zimage "..kernel[1], 0);
  AddLog("Flashed kernel successfully.");
 else
  AddLog("Unknown type for kernel...");
 end
 
 --Finish flash
 
 end
 
 --Start radio flashing
 
 if (selected2==true) then
  AddLog("Flashing radio...");
  CommandLine.Execute(fastboot.." flash radio "..radio[1], 0);
  AddLog("Flashed radio successfully.");
 end
  
  
 if (selected3==true) then
  AddLog("Flashing bootloader...");
  CommandLine.Execute(fastboot.." flash bootloader "..bootloader[1], 0);
  AddLog("Flashed bootloader successfully.");
 end
 
 
 if (selected4==true) then
  AddLog("Flashing recovery...");
  CommandLine.Execute(fastboot.." flash recovery "..recovery[1], 0);
  AddLog("Flashed recovery successfully.");
 end
 
 
 if (selected5==true) then
  AddLog("Flashing system...");
  CommandLine.Execute(fastboot.." flash system "..system[1], 0);
  AddLog("Flashed system successfully.");
 end
 
 if(CheckBox.GetChecked("CheckBox2")=="true") then
  CommandLine.Execute("fastboot reboot", 0);
 end
  

elseif (connected==true and anyselected~=false and RadioButton.GetChecked("RadioButton2")==true) then 
 fastboot=Fastboot()
 
 --Start image flashing algorithm
  AddLog("Flashing image..."); 
  CommandLine.Execute("7za x -o".._TempFolder.."/Flasher "..image[1], 0);
  tgz = String.SplitPath(image[1]);
  imagename = tgz.Filename
  tar2 = String.Replace(tgz.Filename..tgz.Extension, "tgz", "tar", false);
  tar = _TempFolder.."/Flasher/"..tar2
  CommandLine.Execute("7za x -o".._TempFolder.."/Flasher "..tar, 0);
  folder = String.Left(imagename, 12);
  radio2 = File.Find(_TempFolder.."/Flasher/"..folder, "radio*.img", true, false, nil, nil);
  bootloader2 = File.Find(_TempFolder.."/Flasher/"..folder, "bootloader*.img", true, false, nil, nil);
  AddLog("Flashing radio..."); 
  CommandLine.Execute(fastboot.." flash radio "..radio2[1], 0);
  AddLog("Flashing bootloader..."); 
  CommandLine.Execute(fastboot.." flash bootloader "..bootloader2[1], 0);
  zipimage=_TempFolder.."/Flasher/"..folder.."/image-"..folder..".zip"
  AddLog("Flashing main files...");
  CommandLine.Execute("fastboot -w update "..zipimage, 0);
  AddLog("Flashed image successfully.");
 --Finish image flashing algorithm
end
 </Script>
