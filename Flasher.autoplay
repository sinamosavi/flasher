<Event>
<Name>On Preload</Name>
<Args/>
<Script>function DetectKernel(file)
 kernel=GetExtension(file)
 if (kernel.Extension==".img") then
  kerneltype="boot"
 elseif (kernel.Extension==".zip") then
  kerneltype="zip"
 elseif (kernel.Filename=="zImage") then
  kerneltype="zimage"
 else
  kerneltype="unknown"
 end
 return kerneltype
end


function FlashKernel(files)
Dialog.Message("a", files);
 kerneltype=DetectKernel(files)
 if (kerneltype=="boot") then
 Dialog.Message("a","boot");
  AddLog("Flashing kernel...");
  CommandLine.Execute(fastboot.." flash boot "..files, 0);
  AddLog("Flashed kernel successfully.");
 elseif (kerneltype=="zip") then
  AddLog("Flashing kernel...");
  Dialog.Message("a","boot");
  Zip.Extract(files, {"*.*"}, _TempFolder.."/Flasher", true, true, "", ZIP_OVERWRITE_ALWAYS, nil);
  zimage = File.Find(_TempFolder.."/Flasher", "zImage", true, false, nil, nil);
  zimage=zimage[1]
  Dialog.Message("a",fastboot.." flash zimage "..zimage);
  CommandLine.Execute(fastboot.." flash zimage "..zimage, 0);
  AddLog("Flashed kernel successfully.");
  Folder.DeleteTree(_TempFolder.."\\Flasher", nil);
 elseif (kerneltype=="zimage") then
  AddLog("Flashing kernel...");
  Dialog.Message("a","boot");
  CommandLine.Execute(fastboot.." flash zimage "..files, 0);
  AddLog("Flashed kernel successfully.");
 else
  AddLog("Unknown type for kernel...");
 end
end</Script>
