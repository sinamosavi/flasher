<Args/>
<Script>
if (selected~=1) then
result = Dialog.Message("Error", "You have not selected any file to flash", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
end

if (connected~=true) then
result = Dialog.Message("Error", "You have not attached the device", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
end


if (selected == 1) and (type == 1) and (connected==true) then

result="fastboot flash zimage "..result[1];

StatusDlg.Show(MB_ICONNONE, false);
StatusDlg.ShowProgressMeter(false);
StatusDlg.SetTitle("Flashing");
StatusDlg.SetMessage("Flashing zImage...");

a = CommandLine.Execute(result, 20000);

StatusDlg.Hide();

Paragraph.SetText("Paragraph1", a.StdError);
selected=0
Label.SetText("Label2", "File: Nothing yet");

end

if (selected == 1 and type==3 and connected==true) then

result="fastboot flash boot "..result[1];

StatusDlg.Show(MB_ICONNONE, false);
StatusDlg.ShowProgressMeter(false);
StatusDlg.SetTitle("Flashing");
StatusDlg.SetMessage("Flashing kernel...");

a = CommandLine.Execute(result, 20000);

StatusDlg.Hide();

Paragraph.SetText("Paragraph1", a.StdError);
selected=0
Label.SetText("Label2", "File: Nothing yet");

end


if (selected == 1) and (type == 2) and (connected==true) then


Zip.Extract(result[1], {"*.*"}, _TempFolder.."/Flasher", true, true, "", ZIP_OVERWRITE_ALWAYS, nil);
zimage = File.Find(_TempFolder.."/Flasher", "zImage", true, false, nil, nil);
zimage=zimage[1]

result="fastboot flash zimage "..zimage;

StatusDlg.Show(MB_ICONNONE, false);
StatusDlg.ShowProgressMeter(false);
StatusDlg.SetTitle("Flashing");
StatusDlg.SetMessage("Flashing zImage...");

a = CommandLine.Execute(result, 20000);

StatusDlg.Hide();

Paragraph.SetText("Paragraph1", a.StdError);
selected=0
Label.SetText("Label2", "File: Nothing yet");
Folder.DeleteTree(_TempFolder.."\\Flasher", nil);


end


</Script>
