<Event>
<Name>On Preload</Name>
<Args/>
<Script>devices = CommandLine.Execute("fastboot devices", 4000);
search = String.Find(devices.StdOut, "fastboot", 1, false);

if (search~=-1) then

connected=true
Label.SetText("Label3", "Status: Connected");

tLabel3 = Label.GetProperties("Label3");


tLabel3.ColorNormal = Math.HexColorToNumber("0D7309");
tLabel3.ColorDown = Math.HexColorToNumber("0D7309");
tLabel3.ColorHighlight = Math.HexColorToNumber("0D7309");

Label.SetProperties("Label3", tLabel3);


else

connected=false
Label.SetText("Label3", "Status: Not Connected");

tLabel3 = Label.GetProperties("Label3");

tLabel3.ColorNormal = Math.HexColorToNumber("F22121");
tLabel3.ColorDown = Math.HexColorToNumber("F22121");
tLabel3.ColorHighlight = Math.HexColorToNumber("F22121");

Label.SetProperties("Label3", tLabel3);

end


</Script>
